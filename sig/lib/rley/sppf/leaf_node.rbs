module Rley
  module SPPF
    # Abstract class. The generalization for SPPF nodes that don't have
    # child node.
    class LeafNode < SPPFNode
      @key: String

      # @return [String] a text representation of the node.
      def inspect: () -> String

      # @return [String]
      def key: () -> String
    end
  end
end
